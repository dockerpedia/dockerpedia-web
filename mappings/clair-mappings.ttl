#Relation-image->tag
<TriplesMapImage-tag>
  a rr:TriplesMap;

  rr:logicalTable [ rr:sqlQuery """
    SELECT 
      "i"."id" as repository_id,
      "t"."id" as image_id
    FROM image as i
    JOIN tag AS t
      ON t.image_id = i.id
  """;];

  rr:subjectMap [ rr:template "http://dockerpedia.inf.utfsm.cl/resource/Repository/{repository_id}";
    rr:class vocab:Repository];

  rr:predicateObjectMap
  [
    rr:predicate  vocab:hasImage ;
    rr:objectMap  [ rr:template "http://dockerpedia.inf.utfsm.cl/resource/Image/{image_id}" ]
  ];
.


#Relation-image<-tag
<TriplesMapTag-Image>
  a rr:TriplesMap;

  rr:logicalTable [ rr:sqlQuery """
    SELECT 
      "i"."id" as repository_id,
      "t"."id" as image_id
